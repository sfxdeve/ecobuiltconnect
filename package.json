{
	"name": "ecobuiltconnect",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node .output/server/index.mjs",
		"format": "biome format",
		"lint": "biome lint",
		"check": "biome check",
		"db:generate": "dotenvx run -f .env.local -- prisma generate",
		"db:push": "dotenvx run -f .env.local -- prisma db push",
		"db:migrate": "dotenvx run -f .env.local -- prisma migrate dev",
		"db:studio": "dotenvx run -f .env.local -- prisma studio",
		"db:seed": "dotenvx run -f .env.local -- prisma db seed",
		"postinstall": "prisma generate"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.962.0",
